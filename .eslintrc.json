{
  "env": {
      "browser": true
  },
  "extends": [
      "plugin:react/recommended",
      "eslint:recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "react/state-in-constructor": "off",
    "react/destructuring-assignment": "off",
    "react/jsx-filename-extension": "off",
    "class-methods-use-this": "off"
  }
}
